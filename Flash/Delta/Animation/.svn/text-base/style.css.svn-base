#footer {
	position: relative;
    width: 960px;
	height: 100px;
	background: url(images/separator.png) repeat-x;
	margin: -100px auto 0 auto;
	padding: 0;
  	color: #c0c0c0;
	clear: both;
}

#footer .left {
	float: left;
	width: 800px;
	margin: 15px 0 0 0;	
}
#footer ul.links {
	list-style: none;
	width: 800px;
	margin: 0;
	padding: 0;
}
#footer ul.links li a {
	float: left;
	display: block;
	margin: 0 20px 0 0;
	color: #808080;
}
#footer ul.links li a:hover {
	color: #333;
}

#footer .copyright {
	width: 800px;
	margin: 5px 0 15px 0;
}
#footer .right {
	float: right;
	width: 115px;
	margin: 11px 0 0 0;	
}
#footer ul.connect {
	list-style: none;
	width: 115px;
	height: 36px;
	margin: 0;
	padding: 0;
}
#footer ul.connect li a {
	float: left;
	display: block;
	width: 36px;
	height: 36px;
	margin: 0 0 0 2px;
	text-indent: -9999px;
}
#footer ul.connect li.fb a {
	background: transparent url(images/footer_fb.png) no-repeat;
}
#footer ul.connect li.in a {
	background: transparent url(images/footer_in.png) no-repeat;
}
#footer ul.connect li.tw a {
	background: transparent url(images/footer_tw.png) no-repeat;
}
#footer ul.connect li.fb a:hover,
#footer ul.connect li.in a:hover, 
#footer ul.connect li.tw a:hover {
	background-position: 0 -36px;
}





