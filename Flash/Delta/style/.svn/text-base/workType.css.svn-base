html,body {padding:0;margin:0; background-image:url('../images/background_home.png') !important;}

.feedback {
    width:550px;
    text-align:justify;
    text-decoration: none;
    list-style: none;
    font-family: Helvetica Neue;
    font-size: 16px;
    color: #717272;
    text-shadow: 0 1px 0 #FFFFFF;
    line-height:1.6em;
}

.link_div{
    padding: 25px;
}

.link {
    font-family: Helvetica Neue;
    font-size: 20px;
    color: #0000CC;
    text-shadow: 0 1px 0 #FFFFFF;
    line-height:1.6em;
}

pre {
    color: #397D02;
    font-family: Helvetica Neue;
    font-size: 18px;
    font-weight: 700;
    text-shadow: 2px 2px 0 #FFFFFF;
    padding: 15px;
    line-height:1.6em;
    height: 40px;
}

.titles {
    float: left;
    padding-left: 11px;
}

img {
    width: 40px;
}

.button {
    padding: 2px;
}

.button_group {
    float: left;
    padding: 0px 5px 5px 5px;
}

.social_container{
    width: 350px;
    background-color: #CAE1FF;
    padding: 0px 10px 10px 10px;
    height: 105px;
    border-radius: 15px;
}

.feedbackheader {
    font-family: Helvetica Neue;
    text-shadow: 0 1px 0 #FFFFFF;
    font-weight:bolder;
    text-align:center;
    text-align: center;
    /* color: #E63948; */
    color:#89A2B9;
    padding-bottom: 10px;
    margin-top: 60px;
    margin-bottom:0px;
    font-size:36px;
}
















