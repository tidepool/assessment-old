/* BELOW IS THE STYLE FOR THE IFRAME */

html,body {padding:0;margin:0; background-image:url('../images/background2.png') !important;}


#content {margin: auto; width:760px;}
#content li a {text-decoration: none; list-style:none;}

.grid {
width: 67px;
height: 67px;
background-image:url('../images/background3.png');
float: left;
margin-right: 10px;
margin-bottom: 10px;
list-style: none;
border-radius: 4px;
box-shadow:0 1px 0 #FFFFFF;
}

#square_table {
padding: 0px;
margin-top: 60px;
width: 770px;
height: 525px;
}

p#description {
font-family: Helvetica; 
font-size: 18px; 
text-shadow: 0px 1px 0px white;
color:#4B4C58;
font-weight:bolder;
text-align:center;
}

