    font-weight: normal;
    color: #a0a0a0;
}

.form-field {
    margin: 0 0 10px 0;
    padding: 0 5px;
    background: #fafafa;
    border: #b0b0b0 1px solid;
    border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px;
    box-shadow: inset 0 0 3px #d0d0d0; -webkit-box-shadow: inset 0 0 3px #d0d0d0; -moz-box-shadow: inset 0 0 3px #d0d0d0;
    transition: all 0.30s ease-in-out;
    -webkit-transition: all 0.30s ease-in-out;
    -moz-transition: all 0.30s ease-in-out;
    font-family: Helvetica, Arial, sans-serif;
    font-size: 12px;
    font-weight: normal;
    color: #a0a0a0;
}

.form-field:focus,
