
body {
    background: #f0f0f0 url(images/bg_linen.png) repeat;
    margin: 0;
    padding: 0;
    font: 11px/15px Helvetica, Arial, Sans-serif;
    color: #333;
}


/* Common -------------------------------------------------- */

.clear { clear: both; }

a {
    color: #633960;
    text-decoration: none;
    outline: none;
}
a:hover {
    color: #633960;
    text-decoration: underline;
}
a img {
    background: none;
    border-width: 0;
}

h1 {
    width: 960px;
    margin: 0 0 5px 0;
    padding: 0;
    line-height: 35px;
    font-size: 30px;
    font-weight: 300;
    letter-spacing: -2px;
    color: #404040;
    text-align: center;
}
h2 {
    width: 960px;
    margin: 0 0 30px 0;
    padding: 0;
    line-height: 22px;
    font-size: 18px;
    font-weight: 300;
    letter-spacing: -1px;
    color: #909090;
    text-align: center;
}
h3 {
    margin: 0 0 10px 0;
    padding: 0;
    line-height: 25px;
    font-size: 22px;
    font-weight: 300;
    letter-spacing: -1px;
    color: #404040;
}
h4 {
    width: 960px;
    margin: 0 auto 10px auto;
    padding: 0;
    line-height: 35px;
    font-size: 25px;
    font-weight: 300;
    letter-spacing: -1.5px;
    color: #404040;
    text-align: center;
}
h5 {
    width: 960px;
    margin: 0 auto 25px auto;
    padding: 0;
    line-height: 40px;
    font-size: 30px;
    font-weight: 300;
    letter-spacing: -2px;
    color: #404040;
    text-align: center;
}

h6 {
    margin: 0 0 5px 0;
    padding: 0;
    line-height: 35px;
    font-size: 30px;
    font-weight: 300;
    letter-spacing: -2px;
    color: #404040;
    text-align: center;
}
h7 {
    margin: 10px 0 5px 135px;
    padding: 0;
    line-height: 22px;
    font-size: 18px;
    font-weight: bold;
    color: #404040;
}
p {
    margin: 0 0 15px 0;
    padding: 0;
    line-height: 18px;
    font-size: 13px;
}

.separator {
    width: 960px;
    height: 2px;
    background: url(images/separator.png) repeat-x;
    margin: 0 auto;
}

.align-right { text-align: right; }

.nomargin { margin: 0 !important; }

.hdr-spacer { height: 20px; }

div.mess-alert {
    position: relative;
    width: 99%;
    height: 38px;
    padding: 0 0 0 10px;
    border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px;
    line-height: 38px;
    font-size: 12px;
}
div.error {
    background: #F2DEDE;
    border: 1px solid #EFD3D7;
    color: #BA4B49;
}
div.success {
    background: #DFEFD8;
    border: 1px solid #D6E9C6;
    color: #458847;
}
div.note {
    background: #D9ECF6;
    border: 1px solid #BEE6EF;
    color: #3A87AD;
}
span.mess-close a {
    display: block;
    position: absolute;
    top: 9px;
    right: 9px;
    width: 19px;
    height: 19px;
    background: url(images/mess_close.png) no-repeat;
    text-indent: -9999px;
}


/* Structure -------------------------------------------------- */

* { margin: 0; padding: 0; }
html, body { height: 100%; }
#wrap { min-height: 100%; }

#cont {
    position: relative;
    overflow-x: hidden;
    width: 970px;
    margin: 0 auto;
    padding-bottom: 100px;
    overflow: auto;
}


/* Header -------------------------------------------------- */

#header {
    width: 100%;
    margin: 0 auto;
    background: url(images/hdr_bg.png) top repeat-x;
}

.logo a {
    display: block;
    position: relative;
    width: 140px;
    height: 100px;
    margin: 0 auto;
    background: url(images/hdr_logo.png) no-repeat;
    text-indent: -9999px;
    z-index: 20;
}

#header .two-up {
    width: 100%;
    min-width: 960px;
    margin: 0 auto;
    margin-top: -49px;
    font-size: 0;
    text-align: center;
    white-space: nowrap;
}
#header .two-up img {
    display: block;
    width: 100%;
}

#header .two-up .pic-a,
#header .two-up .pic-b {
    display: inline;
}
#header .two-up .pic-a a,
#header .two-up .pic-b a {
    display: inline-block;
    zoom: 1;
    *display: inline;
    width: 48%;
    margin: 0 5px;
    box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.30);
    -moz-box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.30);
    -webkit-box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.30);
}

#header .two-up .pic-a a:hover,
#header .two-up .pic-b a:hover {
    box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.50);
    -moz-box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.50);
    -webkit-box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.50);
    filter: alpha(opacity=90);
    -moz-opacity: 0.9;
    opacity: 0.9;
}

#header .cta-splash {
    position: absolute;
    top: 160px;
    z-index: 10;
}
#header .cta-splash .cta-splash-text {
    width: 560px;
    height: 100px;
    margin: auto;
    background: url(images/cta_splash.png) no-repeat;
}



#header-int {
    width: 100%;
    height: 180px;
    background: url(images/hdr_bg.png) top repeat-x;
}
#header-int .hdr-cont {
    position: relative;
    width: 1000px;
    margin: 0 auto;
}

#header-int .logo a {
    position: absolute;
    top: 0;
    left: 410px;
    width: 180px;
    height: 150px;
    background: url(images/hdr_logo.png) no-repeat;
    text-indent: -9999px;
    z-index:10;
}


/* Splash -------------------------------------------------- */

.members {
    width: 960px;
    margin: 70px 0 30px 0;
}
.members .rl {
    float: left;
    width: 375px;
    height: 2px;
    padding: 8px 0 10px 0;
    background: url(images/separator.png) center center repeat-x;
}
.members .login {
    float: left;
    width: 210px;
    line-height: 22px;
    font-size: 14px;
    color: #909090;
    text-align: center;
}
.members .login a {
    color: #633960;
    text-decoration: none;
}
.members .login a:hover {
    color: #633960;
    text-decoration: underline;
}

.users {
    width: 960px;
    height: 160px;
    margin: 0 0 40px 0;
}

.users .signup {
    float: left;
    width: 188px;
    height: 97px;
    margin-right: 23px;
    padding: 18px 15px 15px 15px;
    background: #e6e6e6;
    border: 1px solid #d0d0d0;
    border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px;
    box-shadow: 0px 1px 0px rgba(255,255,255,1);
    -moz-box-shadow: 0px 1px 0px rgba(255,255,255,1);
    -webkit-box-shadow: 0px 1px 0px rgba(255,255,255,1);
    line-height: 18px;
    font-size: 14px;
    color: #909090;
    text-align: center;
}
.users .signup .signup-btn {
    margin: 20px 0 0 0;
}
.users .signup .signup-btn a {
    display: block;
    width: 100px;
    height: 36px;
    margin-left: 44px;
    background: url(images/btn_signup.png) no-repeat;
    border: none;
    font: 14px Helvetica, Arial, sans-serif;
    color: #ffffff;
    text-shadow: 0px 1px 1px rgba(000,000,000,0.2);
    line-height: 35px;
    outline: none;
    cursor: pointer;
    text-align: center;
}

.users .block img {
    border-top-left-radius: 3px; -moz-border-radius-topleft: 3px; -webkit-border-top-left-radius: 3px;
    border-top-right-radius: 3px; -moz-border-radius-topright: 3px; -webkit-border-top-right-radius: 3px;
}
.users .block a {
    display: block;
    float: left;
    width: 100px;
    height: 130px;
    margin: 0 23px 0 0;
    background: #fff;
    border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px;
    box-shadow: 0px 0px 2px rgba(0, 0, 0, 0.30);
    -moz-box-shadow: 0px 0px 2px rgba(0, 0, 0, 0.30);
    -webkit-box-shadow: 0px 0px 2px rgba(0, 0, 0, 0.30);
    color: #808080;
    text-align: center;
}
.users .block a:hover {
    display: block;
    box-shadow: 0px 0px 3px rgba(0, 0, 0, 0.50);
    -moz-box-shadow: 0px 0px 3px rgba(0, 0, 0, 0.50);
    -webkit-box-shadow: 0px 0px 3px rgba(0, 0, 0, 0.50);
    color: #333;
    text-decoration: none;
}
.users .block a:hover img {
    width: 80px;
    height: 80px;
    padding-top: 10px;
    padding-bottom: 10px;
}
.users .name {
    padding: 0 5px;
    line-height: 30px;
    overflow: hidden;
    white-space: nowrap;
    font-size: 11px;
    z-index: 999;
}


/* Profile -------------------------------------------------- */

div.profile {
